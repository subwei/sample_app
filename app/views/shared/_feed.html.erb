<% if @feed_items.any? %>
  <ol class="microposts">
  	<%# passing collection: @feed_items causes render to use the 'feed_item' partial by convention. In other render invocations, the partial: parameter was omitted. %>
    <%= render partial: 'shared/feed_item', collection: @feed_items %>
  </ol>
  <%= will_paginate @feed_items %>
<% end %>